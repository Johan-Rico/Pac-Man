 class Random {
    static int seed;
    
    function int rand() {
        let seed = seed + 20251;
        if (seed < 0) {
            let seed = seed - 32767 - 1;
        }
        return seed;
    }

    function int randRange(int range) {
        return (Random.rand() - 300 / (32767 / range));
    }

}